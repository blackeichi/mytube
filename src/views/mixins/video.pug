mixin video(Video)
    div.video__form
        a(href ="/videos/" + Video.id)
            div.videoThumbnail(style = `background-image:url(${Video.thumbUrl});background-size:cover; background-position:center;`)
            div.videodata
                span #{Video.title}
        div.videodata__meta
            span #{Video.owner.name}
            span #{Video.meta.views} íšŒ
        small=Video.createdAt 